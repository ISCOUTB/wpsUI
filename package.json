{
  "name": "wellprodsim",
  "version": "0.1.0",
  "description": "Social Simulator used to estimate productivity and well-being of peasant families. It is event oriented, high concurrency, heterogeneous time management and emotional reasoning BDI.",
  "keywords": [
    "electron",
    "next",
    "nextjs",
    "next.js",
    "tailwindcss",
    "tailwind css",
    "typescript",
    "react",
    "reactjs"
  ],
  "main": "main/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"magenta,cyan\" --kill-others \"next dev\" \"electron .\"",
    "build": "next build && electron-builder",
    "start": "next start",
    "electron": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "dist": "next build && next export && electron-builder",
    "lint": "next lint"
  },
  "dependencies": {
    "@electron/remote": "2.1.2",
    "@next/font": "^14.2.14",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@types/node": "^22.7.4",
    "@types/tailwindcss": "^3.0.11",
    "@visx/axis": "^3.10.1",
    "@visx/grid": "^3.5.0",
    "@visx/group": "^3.3.0",
    "@visx/scale": "^3.5.0",
    "@visx/tooltip": "^3.3.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "electron": "^32.1.2",
    "electron-is-dev": "^3.0.1",
    "electron-serve": "^2.0.0",
    "framer-motion": "^11.11.7",
    "lucide-react": "^0.447.0",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.5.3",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "wellprodsim": "file:"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "electron": "30.1.0",
    "electron-builder": "^25.0.0-alpha.9",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.3",
    "tailwindcss": "^3.4.1",
    "typescript": "5.6.2"
  },
  "build": {
    "appId": "WellProdSim",
    "productName": "WellProdSim",
    "files": [
      "out/**/*",
      "main/out/**/*",
      ".next/**/*",
      "main/main.js",
      "main/preload/preload.mjs"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        "dir",
        "portable",
        "zip"
      ],
      "icon": "resources/icon.ico"
    },
    "linux": {
      "target": [
        "dir",
        "appimage",
        "zip"
      ],
      "icon": "resources/icon.png"
    },
    "mac": {
      "target": [
        "dir",
        "dmg",
        "zip"
      ],
      "icon": "resources/icon.icns"
    }
  }
}
